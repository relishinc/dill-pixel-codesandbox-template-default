{
  // These tasks will run in order when initializing your CodeSandbox project.
  "setupTasks": [
    {
      "name": "Install Dependencies",
      "command": "pnpm install"
    }
  ],
  // These tasks can be run from CodeSandbox. Running one will open a log in the app.
  "tasks": {
    "clean": {
      "name": "clean",
      "command": "pnpm clean",
      "runAtStart": false
    },
    "start": {
      "name": "start",
      "command": "pnpm start",
      "runAtStart": false,
      "preview": {
        "port": 5173
      }
    },
    "dev": {
      "name": "dev",
      "command": "pnpm dev",
      "runAtStart": true,
      "preview": {
        "port": 5173
      }
    },
    "prepreview": {
      "name": "prepreview",
      "command": "pnpm prepreview",
      "runAtStart": false
    },
    "preview": {
      "name": "preview",
      "command": "pnpm preview",
      "runAtStart": false
    },
    "prebuild": {
      "name": "prebuild",
      "command": "pnpm prebuild",
      "runAtStart": false
    },
    "build": {
      "name": "build",
      "command": "pnpm build",
      "runAtStart": false
    },
    "assets": {
      "name": "assets",
      "command": "pnpm assets",
      "runAtStart": false
    },
    "test": {
      "name": "test",
      "command": "pnpm test",
      "runAtStart": false
    },
    "postinstall": {
      "name": "postinstall",
      "command": "pnpm postinstall",
      "runAtStart": false
    }
  }
}
